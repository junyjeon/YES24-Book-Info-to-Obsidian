{
  "manifest_version": 3,
  "name": "YES24 Book Info to Obsidian",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["*://*.yes24.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "/images/icon16.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  }
}
